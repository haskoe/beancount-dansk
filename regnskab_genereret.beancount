  WHERE account ~ 'Moms' 
  GROUP BY account
"
1900-01-01 query "forfaldne-fakturaer" "
  SELECT date, description, position, balance 
  WHERE account ~ 'Debitorer' 
  AND meta('due_date') < CAST(today() AS STRING)
"

